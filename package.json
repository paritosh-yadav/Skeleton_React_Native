{
  "name": "SkeletonApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest .*.spec.js",
    "lint": "eslint src/",
    "flow": "flow",
    "precommit": "yarn run lint && yarn run flow",
    "postinstall": "rm -rf .git/hooks/pre-push && rm -rf .git/hooks/pre-commit",
    "ios": "ENVFILE=.env.development react-native run-ios",
    "android": "react-native run-android"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.6",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "axios": "^0.20.0",
    "node-pre-gyp": "^0.13.0",
    "react": "16.13.1",
    "react-native": "0.63.0",
    "react-native-config": "^1.3.1",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-reanimated": "^1.10.2",
    "react-native-safe-area-context": "^3.1.4",
    "react-native-screens": "^2.10.1",
    "react-native-webview": "10.3.1",
    "react-navigation": "^3.11.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-flow": "^7.10.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@testing-library/jest-native": "^3.3.0",
    "@testing-library/react-native": "^7.0.2",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^25.1.0",
    "detox": "^17.4.4",
    "eslint": "^6.5.1",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "flow-bin": "0.122.0",
    "husky": "^3.0.1",
    "jest": "^25.1.0",
    "jest-circus": "^26.4.2",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-devtools": "^4.8.2",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/__tests__/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  }
}